<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.google.s3.member.MemberDAO">
	<update id="memberUpdate" parameterType="com.iu.s3.member.MemberDTO">
		update member set Pw=#{pw}, Name=#{name},Email=#{email},Phone=#{phone} Where Id=#{id}
	</update>
	<delete id="memberDelete" parameterType="com.iu.s3.member.MemberDTO">
		delete member where Id=#{id}
	</delete>
	<insert id="memberJoin" parameterType="com.google.s3.member.MemberDTO">
		insert into member values(#{id}, #{pw}, #{name}, #{email}, #{phone})
	</insert>
	<select id="memberLogin" parameterType="com.google.s3.member.MemberDTO" resultType="com.iu.s3.member.MemberDTO">
		select * from member where Id=#{id} and Pw=#{pw}
	</select>
</mapper>